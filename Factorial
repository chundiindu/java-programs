public class MyClass {
 static int factorial(int n)
 {
     if(n==0)
     {
         return 1;
     }
     else
     {
       return n*factorial(n-1);
     }
 }
 public static void main(String[] args)
 {
     int N = 5;
     System.out.print("Factorial of " + N  + " is " +factorial(5));
 }
    }




public class MyClass {
 public static void main(String[] args)
 {
     int N = 5;
    for(int i=N-1;i>=1;i--)
    {
        N=N*i;
    
     }
     System.out.print(N);
    }
}
