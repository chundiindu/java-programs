import java.util.*;
public class MyClass {
    public static void main(String args[]) {
     int[] arr = {1,2,3,4,5};
     int rotate = 3;
     for(int j=1;j<=rotate;j++)
     {
     
     int first= arr[0];
      int n = arr.length;
     for(int i=0;i< n-1;i++)
     {
         arr[i]=arr[i+1];
     }
     arr[n-1]=first;

     
    }
    System.out.println(Arrays.toString(arr));
}
}
